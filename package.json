{
  "name": "one-piece-discord-bot",
  "version": "1.0.0",
  "description": "A One Piece themed Discord bot that dynamically creates and manages voice channels with authentic sound effects",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "preinstall": "npm config set audit-level moderate",
    "postinstall": "echo '🏴‍☠️ One Piece Bot ready to set sail!'"
  },
  "keywords": ["discord", "bot", "one-piece", "voice-channels", "audio"],
  "author": "ExoCode33",
  "license": "MIT",
  "dependencies": {
    "discord.js": "14.14.1",
    "dotenv": "16.3.1",
    "@discordjs/voice": "0.16.1",
    "prism-media": "1.3.5"
  },
  "optionalDependencies": {
    "sodium": "3.0.2",
    "libsodium-wrappers": "0.7.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "config": {
    "unsafe-perm": true
  }
}
